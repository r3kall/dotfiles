/*--------Importing matugen colors-----*/
@import "shared/colorscheme.rasi"


/*--------Color definitions------------*/

* {
    background:       @surface;
    background-alt:   @surface-container-highest;
    background-alpha: @surface-alpha;
    foreground:       @on-surface;
    border:           @tertiary;
    selected:         @primary-fixed;
    selected-alpha:   @primary-fixed-alpha;
    selected-text:    @surface;
    active:           @tertiary;
    urgent:           @error;
}
